{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "calebporzio/sushi": "^2.4",
    "cyrildewit/eloquent-viewable": "^7.0",
    "guzzlehttp/guzzle": "^7.2",
    "inertiajs/inertia-laravel": "^0.6.9",
    "intervention/image": "^2.7",
    "intervention/imagecache": "^2.6",
    "juststeveking/http-status-code": "^3.0",
    "laravel/fortify": "^1.16",
    "laravel/framework": "^10.0",
    "laravel/sanctum": "^3.2",
    "laravel/scout": "^10.2",
    "laravel/socialite": "^5.8",
    "laravel/telescope": "^4.14",
    "laravel/tinker": "^2.8",
    "predis/predis": "^2.1",
    "spatie/enum": "^3.13",
    "spatie/guzzle-rate-limiter-middleware": "^2.0",
    "spatie/laravel-data": "^3.2",
    "spatie/laravel-enum": "^3.0",
    "spatie/laravel-image-optimizer": "^1.7",
    "spatie/laravel-json-api-paginate": "^1.13",
    "spatie/laravel-query-builder": "^5.2",
    "spatie/laravel-typescript-transformer": "^2.2",
    "spatie/valuestore": "^1.3",
    "swayok/alternative-laravel-cache": "6.1.*",
    "swayok/cache-filesystem-adapter": "^1.0.0",
    "tightenco/ziggy": "^1.5",
    "timacdonald/json-api": "^1.0@beta"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "barryvdh/laravel-ide-helper": "^2.13",
    "fakerphp/faker": "^1.9.1",
    "laravel/pint": "^1.10",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.0",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
